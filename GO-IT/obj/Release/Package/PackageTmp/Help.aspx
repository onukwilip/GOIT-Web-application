    <script>
        function reveal() {
            var Oddsection = document.querySelectorAll('.section:nth-child(odd)');
            for (var i = 0; i < Oddsection.length; i++) {
                var WindowHeight = window.innerHeight;
                var elementTop = Oddsection[i].getBoundingClientRect().top;
                var elementVisible = 150;

                if (elementTop < WindowHeight - elementVisible) {
                    Oddsection[i].classList.add("slideInLeft");
                    Oddsection[i].classList.remove("slideOutLeft");
                }

                else {
                    Oddsection[i].classList.remove("slideInLeft");
                    Oddsection[i].classList.add("slideOutLeft");
                }
            }

            var Evensection = document.querySelectorAll('.section:nth-child(even)');
            for (var i = 0; i < Oddsection.length; i++) {
                var WindowHeight = window.innerHeight;
                var elementTop = Evensection[i].getBoundingClientRect().top;
                var elementVisible = 150;

                if (elementTop < WindowHeight - elementVisible) {
                    Evensection[i].classList.add("slideInRight");
                    Evensection[i].classList.remove("slideOutRight");
                }

                else {
                    Evensection[i].classList.remove("slideInRight");
                    Evensection[i].classList.add("slideOutRight");
                }
            }
        }

        window.addEventListener("scroll", reveal);
        reveal();

        function accordion(activeTab) {
            var tab = document.getElementsByName('accordion-tab');
            var active = document.getElementById(activeTab);

            for (var i = 0; i < tab.length; i++) {
                tab[i].classList.remove('accordion-active');
                tab[i].classList.add('accordion-tab');
            }

            active.classList.remove('accordion-tab');
            active.classList.add('accordion-active');
        }
    </script>
